{
    "manifest_version": 3,
    "name": "Word Wiz",
    "version": "1.0.0",
    "content_scripts": [
      {
        "js": ["contentscript.js"],
        "css": ["contentscript.css"],
        "matches": ["<all_urls>"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs"
  ]
  }